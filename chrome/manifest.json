{
  "manifest_version": 3,
  "name": "ColorFlut",
  "description": "Pick a color",
  "version": "0.98",
  "minimum_chrome_version": "33",
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "permissions": [
    "storage"
  ],
  "sockets": {
    "udp": {
      "send": ["*"],
      "bind": ["*"]
    }
  }
}
